 <div class="d-flex flex-row">
   <div class="card-product">
    <img src="data:image/jpeg;base64,">
    <div class="card-product-infos">
      <h2>Yoga</h2>
      <p>Harmonize your <strong>body</strong> with your  <strong>mind</strong> and <strong>breath.</strong></p>
      <p><strong>Trainer: <%= @activity.equipment %></strong></p>
      <p><strong>Duration :<%= @activity.duration %></strong></p>
    </div>
  </div>

<div class="align-self-baseline">
  <div class="p-2"><h3>Benefits of Yoga:</h3></div>
  <div class="p-2"><p>Can Decrease Stress.</p></div>
  <div class="p-2"><p>Relieves Anxiety.</p></div> 
  <div class="p-2"><p>Could Improve Heart Health.</p></div>
  <div class="p-2"><p>Helps with back pain relief.</p></div>
</div>


<%= simple_form_for [ :employees, @activity, @booking ] do |f| %>
  <%= f.input :date, as: :date, html5: true %>
  <%= f.input :time, as: :time,  html5: true %>
  <%= f.submit "Submit review", class: "btn btn-primary" %>
  

<% end %>



  

